{
  "name": "riki-community",
  "author": "Thomas Flori",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "babel-core": "^6.26.3",
    "jquery": "^3.6.0",
    "materialize-css": "^1.0.0",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/jquery": "^3.5.6",
    "@types/materialize-css": "^1.0.11",
    "@types/moxios": "^0.4.12",
    "@types/node": "^16.9.4",
    "@vue/test-utils": "^1.2.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.11",
    "jest": "^25.5.4",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "moxios": "^0.4.0",
    "node-sass": "^8.0.0",
    "sass-loader": "^12.1.0",
    "ts-jest": "^25.5.1",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10",
    "vue-class-component": "^7.2.6",
    "vue-jest": "^3.0.7",
    "vue-resource": "^1.5.3",
    "vue-template-compiler": "^2.6.14",
    "vue-template-loader": "^1.1.0",
    "vue-template-loader-jest": "^0.1.0",
    "webpack": "^5.53.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "test": "jest --coverage",
    "coveralls": "cat tests/coverage/lcov.info | coveralls"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "html"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.html?$": "vue-template-loader-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "/tests/js/.*\\.spec\\.ts$",
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "moduleNameMapper": {
      "@view/(.*)": "<rootDir>/resources/views/components/$1",
      "@src/(.*)": "<rootDir>/js/$1"
    },
    "coverageReporters": [
      "text",
      "html",
      "lcov"
    ],
    "coverageDirectory": "storage/logs/js-coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ]
  }
}
